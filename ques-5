#include<iostream>
#include<stack>
using namespace std;
bool checkParenthesis(string &s){
    stack<char>st;
   for(char ch:s){
       if(ch=='(' || ch=='[' || ch=='{'){
           st.push(ch);
   }
   else if(ch==')' || ch== ']' || ch=='}'){
       if(st.empty()){
           return false;
       }
       st.pop();
   }

}
   return st.empty();
}
int main(){
string s={")("};
if(checkParenthesis(s)){
    cout<<"it is balanced"<<endl;
}
else
{
    cout<<"Ã¯t is not balanced";
}

}
